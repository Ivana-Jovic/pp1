package rs.ac.bg.etf.pp1;

import rs.ac.bg.etf.pp1.ast.*;
import rs.ac.bg.etf.pp1.ast.VariableArray;
import rs.ac.bg.etf.pp1.ast.VisitorAdaptor;
import rs.etf.pp1.mj.runtime.Code;

public class CodeGenerator extends VisitorAdaptor {
	
	private int mainPC;  
	
	public int getMainPc() {
		return this.mainPC;
	}
	
	
	
//	
	
	
	@Override
	public void visit(MethodTypeNameType methodName){
		Code.put(Code.enter);
	}
	
	@Override
	public void visit(MethodTypeNameVoid methodName){
		Code.put(Code.enter);
		Code.put();//b1 form param - level polje
		Code.put();//b2 form param plus lok
	}
	 
	
	 
}
